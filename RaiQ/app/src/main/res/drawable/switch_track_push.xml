<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/push_back_track_true" />
            <corners android:radius="25dp" />
            <stroke android:color="@color/push_back_track_true_stroke" android:width="0.1dp"/>
            <size android:width="1dp" android:height="2dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/push_back_track_false" />
            <corners android:radius="25dp" />
            <stroke android:color="@color/push_back_track_false_stroke" android:width="0.1dp"/>
            <size android:width="1dp" android:height="2dp" />
        </shape>
    </item>
</selector>